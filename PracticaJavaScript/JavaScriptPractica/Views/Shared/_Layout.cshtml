<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title> @ViewBag.Title - PRACTICAS LPPA Carli</title>
    <link href="~/Content/Site.css" rel="stylesheet" />

</head>
<body>
    <div id="container">

        <!--Cabecera-->
        <header>
            <img class="logo" src="~/Content/img/Logo.gif" alt="logo uai" />
            <nav>
                <ul>
                    <li> <a href="/">Principal</a> </li>
                    <li> <a href="/Home/Contact">Contacto</a> </li>
                    <li> <a href="/Home/About">Acerca de</a></li>
                </ul>
            </nav>
        </header>

        <!--Contenido-->
        <section>
            <article>
                @RenderBody()
            </article>
        </section>

        <!--Contenido Relacionado-->

        <aside id="pub">
            <article>
                <div>
                    <h3>Java Script</h3>
                    <ul>
                        <li><a href="/JavaScript/Index">Principal</a></li>
                        <li><a href="/JavaScript/Variables">Variables</a></li>
                        <li><a href="/JavaScript/Index">Operadores</a></li>
                        <li><a href="/JavaScript/Condicionales">Condicionales</a></li>
                        <li><a href="/JavaScript/Funciones">Funciones</a></li>
                    </ul>
                 </div>
            </article>
        </aside>

        <!--Pie de Pagina-->
        <footer>
            <p>&copy; @DateTime.Now.Year - UAI NORTE - CARLA ROSSI</p>
        </footer>
    </div>
    @RenderSection("scripts", required: false)
</body>
</html>
